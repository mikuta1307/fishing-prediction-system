{
  "version": 2,
  "name": "fishing-prediction-system",
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python@4.2.0",
      "config": {
        "runtime": "python3.12"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "build": {
    "env": {
      "SETUPTOOLS_USE_DISTUTILS": "stdlib",
      "PYTHONPATH": "/var/task"
    }
  },
  "regions": ["hnd1"]
}