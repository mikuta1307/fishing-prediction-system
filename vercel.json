{
  "version": 2,
  "name": "fishing-prediction-system",
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.12",
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "build": {
    "env": {
      "SETUPTOOLS_USE_DISTUTILS": "stdlib",
      "PYTHONPATH": "/var/task"
    }
  },
  "regions": ["hnd1"]
}