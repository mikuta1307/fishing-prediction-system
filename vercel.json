{
  "version": 2,
  "name": "fishing-prediction-system",
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python@3.0.0",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "regions": ["hnd1"]
}