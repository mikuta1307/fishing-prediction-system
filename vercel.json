{
  "version": 2,
  "name": "fishing-prediction-system",
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python@3.0.7",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next@3.8.7"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "regions": ["hnd1"]
}