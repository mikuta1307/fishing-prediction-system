{
  "version": 2,
  "name": "fishing-prediction-system",
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python@2.0.1",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "regions": ["hnd1"]
}